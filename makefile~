
CC=gcc
CFLAGS=-I. -ansi -pedantic -Wall
COFLAGS=-c -I. -ansi -pedantic -Wall

Shell.x: main.o shell_tools.o internal_commands.o procQueue.o
	$(CC) $(CFLAGS) -o Shell.x main.o shell_tools.o internal_commands.o procQueue.o
debug.x: main.o shell_tools.o internal_commands.o procQueue.o
	$(CC) $(CFLAGS) -g -o debug.x main.o shell_tools.o internal_commands.o procQueue.o
main.o: main.c internal_commands.h shell_tools.h procQueue.h
	$(CC) $(COFLAGS) -g -o main.o -c main.c
shell_tools.o: shell_tools.c shell_tools.h
	$(CC) $(COFLAGS) -g -oshell_tools.o shell_tools.c
internal_commands.o: internal_commands.c internal_commands.h shell_tools.h
	$(CC) $(COFLAGS) -g -ointernal_commands.o internal_commands.c
procQueue.o: procQueue.c shell_tools.h procQueue.h
	$(CC) $(COFLAGS) -g -oprocQueue.o procQueue.c
Clean:
	rm -f *.o *.x
